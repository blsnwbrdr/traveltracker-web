{
  "name": "traveltracker-ng-app",
  "version": "1.0.0",
  "scripts": {
    "branch-clean-up": "git branch -lvv | cut -c3- | awk '/: gone]/ {print $1}' | xargs git branch -D",
    "build": "ng build --configuration production --base-href /portfolio/traveltracker/",
    "cy:open": "cypress open",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "ng": "ng",
    "review": "npm run lint && npm run test:headless",
    "start": "ng serve",
    "test": "ng test",
    "test:headless": "ng test --watch=false --browsers=ChromeHeadless"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.1.4",
    "@angular/common": "^18.1.4",
    "@angular/compiler": "^18.1.4",
